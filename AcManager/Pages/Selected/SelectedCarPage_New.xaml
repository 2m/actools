<sel:SelectedAcJsonObjectPage x:Class="AcManager.Pages.Selected.SelectedCarPage_New" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:g="clr-namespace:AcManager" xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        xmlns:sel="clr-namespace:AcManager.Pages.Selected" xmlns:c="http://acstuff.ru/app/controls" xmlns:t="http://acstuff.ru/app/tools" mc:Ignorable="d"
        d:DesignHeight="300" d:DesignWidth="300" mui:InputBindingBehavior.PropagateInputBindingsToWindow="True" FocusVisualStyle="{x:Null}"
        d:DataContext="{d:DesignInstance sel:SelectedCarPage_New+ViewModel}">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <mui:SharedResourceDictionary Source="/AcManager.Controls;component/Assets/AcSelectedSpecific.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <CollectionViewSource x:Key="ShowroomPresetsItems" Source="{Binding ShowroomPresets}" />
            <CollectionViewSource x:Key="QuickDrivePresetsItems" Source="{Binding QuickDrivePresets}" />
            <CollectionViewSource x:Key="UpdatePreviewsPresetsItems" Source="{Binding UpdatePreviewsPresets}" />

            <mui:BindingProxy x:Key="Model" Data="{Binding}" />
            <mui:BindingProxy x:Key="Selected" Data="{Binding SelectedObject}" />
        </ResourceDictionary>
    </UserControl.Resources>

    <c:AcObjectBase AcObject="{Binding SelectedObject}" ShowIcon="True" IconMouseDown="AcObjectBase_IconMouseDown" Icon="{Binding SelectedObject.BrandBadge}">
        <c:AcObjectBase.ToolBars>
            <ToolBar ToolBarTray.IsLocked="True" Band="0" BandIndex="0">
                <Button Content="{x:Static g:AppStrings.Toolbar_Folder}" Command="{Binding SelectedObject.ViewInExplorerCommand}"
                        ToolTip="{x:Static g:AppStrings.Toolbar_Folder_CarTooltip}" />
                <Button Content="{x:Static g:AppStrings.Toolbar_Reload}" ToolTip="{x:Static g:AppStrings.Toolbar_Reload_Tooltip}"
                        Command="{Binding SelectedObject.ReloadCommand}" CommandParameter="full" />
                <Separator />
                <mui:ButtonWithComboBox Content="{x:Static g:AppStrings.Toolbar_Showroom}" ButtonToolTip="{x:Static g:AppStrings.Toolbar_Showroom_Tooltip}"
                        Command="{Binding OpenInShowroomCommand}" PreviewMouseDown="ToolbarButtonShowroom_OnPreviewMouseDown">
                    <MenuItem Header="{x:Static c:ControlsStrings.Common_Options}" ToolTip="{x:Static g:AppStrings.Toolbar_ShowroomOptions_Tooltip}"
                            InputGestureText="Ctrl+Shift+H" Command="{Binding OpenInShowroomOptionsCommand}" />
                    <MenuItem Header="{x:Static c:ControlsStrings.Common_Presets}">
                        <MenuItem.ItemsSource>
                            <CompositeCollection>
                                <CollectionContainer Collection="{Binding Source={StaticResource ShowroomPresetsItems}}" />
                            </CompositeCollection>
                        </MenuItem.ItemsSource>
                    </MenuItem>
                    <MenuItem Header="{x:Static g:AppStrings.Toolbar_CustomShowroom}">
                        <MenuItem x:Name="LiteCustomShowroomMenuItem" Header="{x:Static g:AppStrings.Toolbar_CustomShowroom_Lite}"
                                ToolTip="{x:Static g:AppStrings.Toolbar_CustomShowroom_Lite_Tooltip}" Command="{Binding OpenInCustomShowroomCommand}"
                                CommandParameter="{x:Static c:CustomShowroomMode.Lite}" />
                        <MenuItem x:Name="FancyCustomShowroomMenuItem" Header="{x:Static g:AppStrings.Toolbar_CustomShowroom_Fancy}"
                                ToolTip="{x:Static g:AppStrings.Toolbar_CustomShowroom_Fancy_Tooltip}" Command="{Binding OpenInCustomShowroomCommand}"
                                CommandParameter="{x:Static c:CustomShowroomMode.Fancy}" />
                    </MenuItem>
                </mui:ButtonWithComboBox>
                <mui:ButtonWithComboBox Content="{x:Static g:AppStrings.Toolbar_Drive}" ButtonToolTip="{x:Static g:AppStrings.Toolbar_Drive_CarTooltip}"
                        Command="{Binding DriveCommand}" PreviewMouseDown="ToolbarButtonQuickDrive_OnPreviewMouseDown"
                        Style="{StaticResource Go.ToolBarButtonWithComboBox}">
                    <mui:ButtonWithComboBox.MenuItems>
                        <CompositeCollection>
                            <CollectionContainer Collection="{Binding Source={StaticResource QuickDrivePresetsItems}}" />
                        </CompositeCollection>
                    </mui:ButtonWithComboBox.MenuItems>
                </mui:ButtonWithComboBox>
                <Separator />
                <Button Content="{x:Static g:AppStrings.Toolbar_Analyze}" IsEnabled="False" />
                <Separator />
                <Button Content="{x:Static g:AppStrings.Toolbar_CarSkins}" Command="{Binding ManageSkinsCommand}"
                        ToolTip="{x:Static g:AppStrings.Toolbar_CarSkins_Tooltip}" />
                <Button Content="{x:Static g:AppStrings.Toolbar_CarSetups}" Command="{Binding ManageSetupsCommand}"
                        ToolTip="{x:Static g:AppStrings.Toolbar_CarSetups_Tooltip}" />
            </ToolBar>
            <ToolBar Band="1" BandIndex="1">
                <Button Content="{x:Static g:AppStrings.Toolbar_Disable}" Command="{Binding SelectedObject.ToggleCommand}"
                        ToolTip="{x:Static g:AppStrings.Toolbar_Disable_Tooltip}"
                        Visibility="{Binding SelectedObject.Enabled, Converter={StaticResource BooleanToVisibilityConverter}}" />
                <Button Content="{x:Static g:AppStrings.Toolbar_Enable}" Command="{Binding SelectedObject.ToggleCommand}"
                        ToolTip="{x:Static g:AppStrings.Toolbar_Enable_Tooltip}"
                        Visibility="{Binding SelectedObject.Enabled, Converter={StaticResource BooleanToVisibilityConverter}, ConverterParameter=inverse}" />
                <Button Content="{x:Static g:AppStrings.Toolbar_Save}" Command="{Binding SelectedObject.SaveCommand}"
                        ToolTip="{x:Static g:AppStrings.Toolbar_Save_JsonTooltip}" />
                <Button Content="{x:Static g:AppStrings.Toolbar_Delete}" Command="{Binding SelectedObject.DeleteCommand}"
                        ToolTip="{x:Static g:AppStrings.Toolbar_Delete_CarTooltip}" />
                <Separator />
                <Button Content="{x:Static g:AppStrings.Toolbar_FindInformation}" Command="{Binding FindInformationCommand}"
                        ToolTip="{x:Static g:AppStrings.Toolbar_FindInformation_CarTooltip}" />
                <mui:ButtonWithComboBox Content="{x:Static g:AppStrings.Toolbar_UpdatePreviews}" Command="{Binding UpdatePreviewsCommand}"
                        ButtonToolTip="{x:Static g:AppStrings.Toolbar_UpdatePreviews_Tooltip}" PreviewMouseDown="ToolbarButtonUpdatePreviews_OnPreviewMouseDown">
                    <MenuItem Header="{x:Static g:AppStrings.Toolbar_UpdatePreviewsManually}"
                            ToolTip="{x:Static g:AppStrings.Toolbar_UpdatePreviewsManually_Tooltip}" InputGestureText="Ctrl+Alt+P"
                            Command="{Binding UpdatePreviewsManuallyCommand}" />
                    <MenuItem Header="{x:Static c:ControlsStrings.Common_Options}" ToolTip="{x:Static g:AppStrings.Toolbar_UpdatePreviewsOptions_Tooltip}"
                            InputGestureText="Ctrl+Shift+P" Command="{Binding UpdatePreviewsOptionsCommand}" />
                    <Separator />
                    <MenuItem Header="{x:Static c:ControlsStrings.Common_Presets}">
                        <MenuItem.ItemsSource>
                            <CompositeCollection>
                                <CollectionContainer Collection="{Binding Source={StaticResource UpdatePreviewsPresetsItems}}" />
                            </CompositeCollection>
                        </MenuItem.ItemsSource>
                    </MenuItem>
                </mui:ButtonWithComboBox>
            </ToolBar>
            <ToolBar Band="1" BandIndex="1">
                <Button Content="{x:Static g:AppStrings.Toolbar_PackData}" Command="{Binding PackDataCommand}"
                        ToolTip="{x:Static g:AppStrings.Toolbar_PackData_Tooltip}"
                        Visibility="{Binding DeveloperMode, Source={x:Static t:SettingsHolder.Common}, Converter={StaticResource BooleanToVisibilityConverter}}" />
                <Button Content="{x:Static g:AppStrings.Toolbar_UnpackData}" Command="{Binding ReadDataCommand}"
                        ToolTip="{x:Static g:AppStrings.Toolbar_UnpackData_Tooltip}"
                        Visibility="{Binding MsMode, Source={x:Static t:SettingsHolder.Common}, Converter={StaticResource BooleanToVisibilityConverter}}" />
                <Button Content="{x:Static g:AppStrings.Toolbar_ReplaceSound}" Command="{Binding ReplaceSoundCommand}"
                        ToolTip="{x:Static g:AppStrings.Toolbar_ReplaceSound_Tooltip}" />
                <Button Content="Replace Tyres" Command="{Binding ReplaceTyresCommand}"
                        Visibility="{Binding MsMode, Source={x:Static t:SettingsHolder.Common}, Converter={StaticResource BooleanToVisibilityConverter}}" />
            </ToolBar>
        </c:AcObjectBase.ToolBars>

        <StackPanel>
            <Grid Margin="0 0 0 12">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="2*" MaxWidth="800" />
                    <ColumnDefinition Width="1*" MinWidth="240" />
                </Grid.ColumnDefinitions>

                <mui:Cell MaxWidth="800">
                    <mui:BetterImage x:Name="SelectedSkinPreviewImage" Filename="{Binding SelectedObject.SelectedSkin.PreviewImage}"
                            PreviewMouseLeftButtonDown="SelectedSkinPreview_MouseDown" PreviewMouseRightButtonUp="SelectedSkinPreview_MouseUp"
                            Height="{Binding ActualWidth, RelativeSource={RelativeSource Self}, Converter={StaticResource MultiplyConverter}, ConverterParameter=0.5626223}"
                            MaxWidth="800" />
                    <ListBox ItemsSource="{Binding SelectedObject.SkinsEnabledWrappersList}" ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                            IsSynchronizedWithCurrentItem="True" Margin="4" VerticalAlignment="Top" MaxHeight="76" BorderThickness="0"
                            ScrollViewer.CanContentScroll="True">
                        <ListBox.ItemsPanel>
                            <ItemsPanelTemplate>
                                <mui:VirtualizingTilePanel HorizontalAlignment="Left" VirtualizationMode="Recycling" IsVirtualizing="True" ScrollUnit="Pixel"
                                        ItemWidth="36" ItemHeight="36" />
                            </ItemsPanelTemplate>
                        </ListBox.ItemsPanel>
                        <ListBox.ItemTemplate>
                            <DataTemplate DataType="{x:Type t:AcItemWrapper}">
                                <!-- ReSharper disable once Xaml.BindingWithContextNotResolved -->
                                <mui:BetterImage ToolTip="{Binding Value.DisplayName}" Filename="{Binding Value.LiveryImage}" Width="32" Height="32"
                                        ClearOnChange="True" PreviewMouseRightButtonUp="SelectedSkinPreview_MouseUp" />
                            </DataTemplate>
                        </ListBox.ItemTemplate>
                    </ListBox>
                    <mui:BetterImage Margin="8" HorizontalAlignment="Right" VerticalAlignment="Bottom" Width="64" Height="64" MouseDown="UpgradeIcon_MouseDown"
                            Filename="{Binding SelectedObject.UpgradeIcon}"
                            Visibility="{Binding SelectedObject.IsChild, Converter={StaticResource BooleanToVisibilityConverter}}" />
                </mui:Cell>

                <!-- description -->
                <mui:BetterTextBox Style="{StaticResource Borderless}" Text="{Binding SelectedObject.Description}" AcceptsReturn="True" TextWrapping="Wrap"
                        Grid.Column="1" Height="{Binding ActualHeight, Source={x:Reference SelectedSkinPreviewImage}}" Margin="20 0 0 0" />
            </Grid>

            <!-- data section -->
            <StackPanel Visibility="{Binding SelectedObject.HasData, Converter={StaticResource BooleanToVisibilityConverter}}">
                <!-- tags -->
                <mui:TagsList ItemsSource="{Binding SelectedObject.Tags}" SuggestionsSource="{x:Static t:SuggestionLists.CarTagsListView}"
                        mui:ContextMenuAdvancement.PropagateToChildren="True" Margin="0 0 0 12">
                    <mui:TagsList.ItemContextMenu>
                        <mui:InheritingContextMenu>
                            <MenuItem Header="{x:Static g:AppStrings.Filter_ByTag}" Command="{Binding Data.FilterTagCommand, Source={StaticResource Model}}"
                                    CommandParameter="{Binding}" />
                        </mui:InheritingContextMenu>
                    </mui:TagsList.ItemContextMenu>
                    <mui:TagsList.ContextMenu>
                        <ContextMenu>
                            <MenuItem Header="{x:Static g:AppStrings.Tags_CleanUp}"
                                    Command="{Binding Data.TagsCleanUpCommand, Source={StaticResource Selected}}" />
                            <MenuItem Header="{x:Static g:AppStrings.Tags_Sort}" Command="{Binding Data.TagsSortCommand, Source={StaticResource Selected}}" />
                            <MenuItem Header="{x:Static g:AppStrings.Tags_CleanUpAndSort}"
                                    Command="{Binding Data.TagsCleanUpAndSortCommand, Source={StaticResource Selected}}" InputGestureText="Ctrl+Alt+T" />
                        </ContextMenu>
                    </mui:TagsList.ContextMenu>
                </mui:TagsList>

                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="360" />
                        <ColumnDefinition Width="1*" />
                        <ColumnDefinition Width="1*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="1*" />
                        <RowDefinition Height="1*" />
                    </Grid.RowDefinitions>

                    <c:OxyPlotGraphViewer Grid.RowSpan="2" SourceTorque="{Binding SelectedObject.SpecsTorqueCurve}"
                            SourcePower="{Binding SelectedObject.SpecsPowerCurve}" Height="200" HorizontalAlignment="Stretch" />

                    <c:PropertiesGrid Columns="1" Grid.Column="1" Grid.Row="0">
                        <mui:BetterComboBox c:PropertiesGrid.Label="{x:Static c:ControlsStrings.AcObject_CarBrandLabel}"
                                Text="{Binding SelectedObject.Brand, UpdateSourceTrigger=LostFocus}"
                                ItemsSource="{x:Static t:SuggestionLists.CarBrandsListView}" Style="{StaticResource BetterComboBox.Borderless}"
                                mui:ContextMenuAdvancement.PropagateToChildren="True">
                            <mui:BetterComboBox.ContextMenu>
                                <ContextMenu>
                                    <MenuItem Header="{x:Static g:AppStrings.Filter_ByBrand}"
                                            Command="{Binding Data.FilterCommand, Source={StaticResource Model}}" CommandParameter="brand" />
                                </ContextMenu>
                            </mui:BetterComboBox.ContextMenu>
                        </mui:BetterComboBox>

                        <mui:BetterComboBox c:PropertiesGrid.Label="{x:Static c:ControlsStrings.AcObject_ContryLabel}"
                                Text="{Binding SelectedObject.Country, UpdateSourceTrigger=LostFocus}"
                                ItemsSource="{x:Static t:SuggestionLists.CountriesListView}" Style="{StaticResource BetterComboBox.Borderless}"
                                mui:ContextMenuAdvancement.PropagateToChildren="True">
                            <mui:BetterComboBox.ContextMenu>
                                <ContextMenu>
                                    <MenuItem Header="{x:Static g:AppStrings.Filter_ByCountry}"
                                            Command="{Binding Data.FilterCommand, Source={StaticResource Model}}" CommandParameter="country" />
                                </ContextMenu>
                            </mui:BetterComboBox.ContextMenu>
                        </mui:BetterComboBox>

                        <mui:BetterComboBox c:PropertiesGrid.Label="{x:Static c:ControlsStrings.AcObject_CarClassLabel}"
                                Text="{Binding SelectedObject.CarClass, UpdateSourceTrigger=LostFocus}"
                                ItemsSource="{x:Static t:SuggestionLists.CarClassesListView}" Style="{StaticResource BetterComboBox.Borderless}"
                                mui:ContextMenuAdvancement.PropagateToChildren="True">
                            <mui:BetterComboBox.ToolTip>Car class (such as “race” or “street”) in lowercase</mui:BetterComboBox.ToolTip>
                            <mui:BetterComboBox.ContextMenu>
                                <ContextMenu>
                                    <MenuItem Header="{x:Static g:AppStrings.Filter_ByClass}"
                                            Command="{Binding Data.FilterCommand, Source={StaticResource Model}}" CommandParameter="class" />
                                </ContextMenu>
                            </mui:BetterComboBox.ContextMenu>
                        </mui:BetterComboBox>

                        <Border c:PropertiesGrid.Label="{x:Static c:ControlsStrings.AcObject_YearLabel}"
                                ToolTip="{x:Static c:ControlsStrings.AcObject_YearLabel_CarTooltip}" mui:ContextMenuAdvancement.PropagateToChildren="True">
                            <Border.ContextMenu>
                                <ContextMenu>
                                    <MenuItem Header="{x:Static g:AppStrings.Filter_ByYear}"
                                            Command="{Binding Data.FilterCommand, Source={StaticResource Model}}" CommandParameter="year" />
                                    <MenuItem Header="{x:Static g:AppStrings.Filter_ByDecade}"
                                            Command="{Binding Data.FilterCommand, Source={StaticResource Model}}" CommandParameter="decade" />
                                </ContextMenu>
                            </Border.ContextMenu>
                            <mui:BetterTextBox Text="{Binding SelectedObject.Year, TargetNullValue=''}" Mode="Integer" Minimum="1900" Maximum="2100"
                                    Style="{StaticResource Borderless}" />
                        </Border>

                        <mui:BbCodeBlock c:PropertiesGrid.Label="{x:Static c:ControlsStrings.AcObject_CarParentLabel}"
                                Text="{Binding SelectedObject.ParentDisplayName}" Placeholder="{x:Static t:ToolsStrings.Common_None}"
                                MouseDown="ParentBlock_OnMouseDown">
                            <mui:BbCodeBlock.ToolTip>
                                <mui:ReferenceSwitch Value="{Binding SelectedObject.Parent}">
                                    <mui:ReferenceSwitch.Null>
                                        <TextBlock Text="{x:Static c:ControlsStrings.AcObject_CarParentLabel_Tooltip}" />
                                    </mui:ReferenceSwitch.Null>
                                    <mui:ReferenceSwitch.NonNull>
                                        <StackPanel Margin="4 8">
                                            <TextBlock Style="{StaticResource Heading2}" Text="{Binding SelectedObject.Parent.DisplayName}" Margin="0 0 0 4" />
                                            <c:CarBlock Car="{Binding SelectedObject.Parent}" ShowSkinsAndPreview="False" SelectSkin="False" Width="512"
                                                    MaxHeight="320" />
                                        </StackPanel>
                                    </mui:ReferenceSwitch.NonNull>
                                </mui:ReferenceSwitch>
                            </mui:BbCodeBlock.ToolTip>
                        </mui:BbCodeBlock>

                        <mui:BbCodeBlock ToolTip="{x:Static c:ControlsStrings.AcObject_Author_Tooltip}" BbCode="{Binding SelectedObject.VersionInfoDisplay}"
                                Placeholder="?" MouseUp="OnVersionInfoBlockClick" mui:ContextMenuAdvancement.PropagateToChildren="True" TextWrapping="NoWrap"
                                TextTrimming="CharacterEllipsis">
                            <c:PropertiesGrid.Label>
                                <MultiBinding Converter="{x:Static sel:SelectedAcObjectViewModel.VersionInfoLabelConverter}">
                                    <Binding Path="SelectedObject" />
                                    <Binding Path="SelectedObject.VersionInfoDisplay" />
                                </MultiBinding>
                            </c:PropertiesGrid.Label>
                            <mui:BbCodeBlock.ContextMenu>
                                <ContextMenu>
                                    <MenuItem Header="{x:Static g:AppStrings.Filter_ByAuthor}"
                                            Command="{Binding Data.FilterCommand, Source={StaticResource Model}}" CommandParameter="author" />
                                </ContextMenu>
                            </mui:BbCodeBlock.ContextMenu>
                        </mui:BbCodeBlock>
                    </c:PropertiesGrid>

                    <c:PropertiesGrid Columns="1" Grid.Column="1" Grid.Row="1" Margin="0 8 0 0">
                        <mui:BetterTextBox c:PropertiesGrid.Label="{x:Static g:AppStrings.CarSpecs_PowerLabel}"
                                Text="{Binding SelectedObject.SpecsBhp, Mode=OneWay}" Style="{StaticResource Borderless}"
                                           Mode="Integer"
                                Placeholder="{x:Static g:AppStrings.CarSpecs_Power_FormatTooltip}">
                            <mui:BetterTextBox.ToolTip>
                                <ToolTip Content="{x:Static g:AppStrings.CarSpecs_Power_FormatTooltip}"
                                        ContentStringFormat="{x:Static g:AppStrings.CarSpecs_Power_Tooltip}" />
                            </mui:BetterTextBox.ToolTip>
                        </mui:BetterTextBox>
                        <mui:BetterTextBox c:PropertiesGrid.Label="{x:Static g:AppStrings.CarSpecs_TorqueLabel}" Text="{Binding SelectedObject.SpecsTorque}"
                                Style="{StaticResource Borderless}" Mode="Integer"
                                Placeholder="{x:Static g:AppStrings.CarSpecs_Torque_FormatTooltip}">
                            <mui:BetterTextBox.ToolTip>
                                <ToolTip Content="{x:Static g:AppStrings.CarSpecs_Torque_FormatTooltip}"
                                        ContentStringFormat="{x:Static g:AppStrings.CarSpecs_Torque_Tooltip}" />
                            </mui:BetterTextBox.ToolTip>
                        </mui:BetterTextBox>
                        <mui:BetterTextBox c:PropertiesGrid.Label="{x:Static g:AppStrings.CarSpecs_WeightLabel}"
                                Text="{Binding SelectedObject.SpecsWeight, Mode=OneWay}" Style="{StaticResource Borderless}"
                                           Mode="Integer"
                                Placeholder="{x:Static g:AppStrings.CarSpecs_Weight_FormatTooltip}">
                            <mui:BetterTextBox.ToolTip>
                                <ToolTip Content="{x:Static g:AppStrings.CarSpecs_Weight_FormatTooltip}"
                                        ContentStringFormat="{x:Static g:AppStrings.CarSpecs_Weight_Tooltip}" />
                            </mui:BetterTextBox.ToolTip>
                        </mui:BetterTextBox>

                        <mui:BetterTextBox c:PropertiesGrid.Label="{x:Static g:AppStrings.CarSpecs_MaxSpeedLabel}"
                                Text="{Binding SelectedObject.SpecsTopSpeed, Mode=OneWay}" Style="{StaticResource Borderless}"
                                           Mode="Integer"
                                Placeholder="{x:Static g:AppStrings.CarSpecs_MaxSpeed_FormatTooltip}">
                            <mui:BetterTextBox.ToolTip>
                                <ToolTip Content="{x:Static g:AppStrings.CarSpecs_MaxSpeed_FormatTooltip}"
                                        ContentStringFormat="{x:Static g:AppStrings.CarSpecs_MaxSpeed_Tooltip}" />
                            </mui:BetterTextBox.ToolTip>
                        </mui:BetterTextBox>
                        <mui:BetterTextBox c:PropertiesGrid.Label="{x:Static g:AppStrings.CarSpecs_AccelerationLabel}"
                                Text="{Binding SelectedObject.SpecsAcceleration}" Style="{StaticResource Borderless}"
                                           Mode="Number"
                                Placeholder="{x:Static g:AppStrings.CarSpecs_Acceleration_FormatTooltip}">
                            <mui:BetterTextBox.ToolTip>
                                <ToolTip Content="{x:Static g:AppStrings.CarSpecs_Acceleration_FormatTooltip}"
                                        ContentStringFormat="{x:Static g:AppStrings.CarSpecs_Acceleration_Tooltip}" />
                            </mui:BetterTextBox.ToolTip>
                        </mui:BetterTextBox>
                        <mui:BetterTextBox c:PropertiesGrid.Label="{x:Static g:AppStrings.CarSpecs_PwRatioLabel}"
                                Text="{Binding SelectedObject.SpecsPwRatio, Mode=OneWay}" Style="{StaticResource Borderless}"
                                           Mode="Number"
                                Placeholder="{x:Static g:AppStrings.CarSpecs_PwRatio_FormatTooltip}">
                            <mui:BetterTextBox.ToolTip>
                                <ToolTip Content="{x:Static g:AppStrings.CarSpecs_PwRatio_FormatTooltip}"
                                        ContentStringFormat="{x:Static g:AppStrings.CarSpecs_PwRatio_Tooltip}" />
                            </mui:BetterTextBox.ToolTip>
                        </mui:BetterTextBox>
                    </c:PropertiesGrid>
                </Grid>
            </StackPanel>
        </StackPanel>
    </c:AcObjectBase>
</sel:SelectedAcJsonObjectPage>

