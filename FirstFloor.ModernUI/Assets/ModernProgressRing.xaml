<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:c="clr-namespace:FirstFloor.ModernUI.Windows.Controls" xmlns:sys="clr-namespace:System;assembly=mscorlib">
  <Style x:Key="BallWithPlatformProgressRingStyle" TargetType="c:ModernProgressRing">
    <Setter Property="Background" Value="Transparent" />
    <Setter Property="Foreground" Value="{DynamicResource Accent}" />
    <Setter Property="IsHitTestVisible" Value="False" />
    <Setter Property="HorizontalAlignment" Value="Center" />
    <Setter Property="VerticalAlignment" Value="Center" />
    <Setter Property="Height" Value="80" />
    <Setter Property="Width" Value="80" />
    <Setter Property="IsTabStop" Value="False" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="c:ModernProgressRing">
          <Border x:Name="PART_Border" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}">
            <VisualStateManager.VisualStateGroups>
              <VisualStateGroup x:Name="SizeStates">
                <VisualState x:Name="Large" />
                <VisualState x:Name="Small" />
              </VisualStateGroup>
              <VisualStateGroup x:Name="ActiveStates">
                <VisualState x:Name="Inactive" />
                <VisualState x:Name="Active">
                  <Storyboard SpeedRatio="1.2">
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetName="PART_Platform" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(RotateTransform.Angle)">
                      <EasingDoubleKeyFrame KeyTime="0:0:0.000" Value="7">
                        <EasingDoubleKeyFrame.EasingFunction>
                          <QuarticEase EasingMode="EaseInOut" />
                        </EasingDoubleKeyFrame.EasingFunction>
                      </EasingDoubleKeyFrame>
                      <EasingDoubleKeyFrame KeyTime="0:0:1.000" Value="-187">
                        <EasingDoubleKeyFrame.EasingFunction>
                          <QuarticEase EasingMode="EaseInOut" />
                        </EasingDoubleKeyFrame.EasingFunction>
                      </EasingDoubleKeyFrame>
                      <EasingDoubleKeyFrame KeyTime="0:0:2.000" Value="7">
                        <EasingDoubleKeyFrame.EasingFunction>
                          <QuarticEase EasingMode="EaseInOut" />
                        </EasingDoubleKeyFrame.EasingFunction>
                      </EasingDoubleKeyFrame>
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetName="PART_Platform" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[1].(TranslateTransform.X)">
                      <LinearDoubleKeyFrame KeyTime="0:0:0.000" Value="20" />
                      <LinearDoubleKeyFrame KeyTime="0:0:1.000" Value="60" />
                      <LinearDoubleKeyFrame KeyTime="0:0:2.000" Value="20" />
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetName="PART_Ball" Storyboard.TargetProperty="(UIElement.RenderTransform).(TranslateTransform.X)">
                      <LinearDoubleKeyFrame KeyTime="0:0:0.000" Value="22" />
                      <LinearDoubleKeyFrame KeyTime="0:0:1.000" Value="58" />
                      <LinearDoubleKeyFrame KeyTime="0:0:2.000" Value="22" />
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetName="PART_Ball" Storyboard.TargetProperty="(UIElement.RenderTransform).(TranslateTransform.Y)">
                      <LinearDoubleKeyFrame KeyTime="0:0:0.000" Value="45" />
                      <EasingDoubleKeyFrame KeyTime="0:0:0.500" Value="14">
                        <EasingDoubleKeyFrame.EasingFunction>
                          <QuadraticEase EasingMode="EaseOut" />
                        </EasingDoubleKeyFrame.EasingFunction>
                      </EasingDoubleKeyFrame>
                      <EasingDoubleKeyFrame KeyTime="0:0:1.000" Value="45">
                        <EasingDoubleKeyFrame.EasingFunction>
                          <QuadraticEase EasingMode="EaseIn" />
                        </EasingDoubleKeyFrame.EasingFunction>
                      </EasingDoubleKeyFrame>
                      <EasingDoubleKeyFrame KeyTime="0:0:1.500" Value="14">
                        <EasingDoubleKeyFrame.EasingFunction>
                          <QuadraticEase EasingMode="EaseOut" />
                        </EasingDoubleKeyFrame.EasingFunction>
                      </EasingDoubleKeyFrame>
                      <EasingDoubleKeyFrame KeyTime="0:0:2.000" Value="45">
                        <EasingDoubleKeyFrame.EasingFunction>
                          <QuadraticEase EasingMode="EaseIn" />
                        </EasingDoubleKeyFrame.EasingFunction>
                      </EasingDoubleKeyFrame>
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetName="PART_Hit0" Duration="0:0:2" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)">
                      <DiscreteDoubleKeyFrame KeyTime="0:0:1.000" Value="0" />
                      <LinearDoubleKeyFrame KeyTime="0:0:1.300" Value="2" />
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetName="PART_Hit0" Duration="0:0:2" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)">
                      <DiscreteDoubleKeyFrame KeyTime="0:0:1.000" Value="0" />
                      <LinearDoubleKeyFrame KeyTime="0:0:1.300" Value="2" />
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetName="PART_Hit0" Duration="0:0:2" Storyboard.TargetProperty="(UIElement.Opacity)">
                      <DiscreteDoubleKeyFrame KeyTime="0:0:1.000" Value="0.5" />
                      <LinearDoubleKeyFrame KeyTime="0:0:1.300" Value="0" />
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetName="PART_Hit1" Duration="0:0:2" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)">
                      <DiscreteDoubleKeyFrame KeyTime="0:0:0.000" Value="0" />
                      <LinearDoubleKeyFrame KeyTime="0:0:0.300" Value="2" />
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetName="PART_Hit1" Duration="0:0:2" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)">
                      <DiscreteDoubleKeyFrame KeyTime="0:0:0.000" Value="0" />
                      <LinearDoubleKeyFrame KeyTime="0:0:0.300" Value="2" />
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetName="PART_Hit1" Duration="0:0:2" Storyboard.TargetProperty="(UIElement.Opacity)">
                      <DiscreteDoubleKeyFrame KeyTime="0:0:0.000" Value="0.5" />
                      <LinearDoubleKeyFrame KeyTime="0:0:0.300" Value="0" />
                    </DoubleAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Canvas>
              <Rectangle x:Name="PART_Platform" Width="40" Height="8" Fill="{TemplateBinding Foreground}" RenderTransformOrigin="0.5, 0.5" Margin="-20 0">
                <Rectangle.RenderTransform>
                  <TransformGroup>
                    <RotateTransform />
                    <TranslateTransform Y="54" />
                  </TransformGroup>
                </Rectangle.RenderTransform>
              </Rectangle>
              <Ellipse x:Name="PART_Ball" Width="20" Height="20" Fill="{TemplateBinding Foreground}" RenderTransformOrigin="0.5, 0.5" Margin="-10">
                <Ellipse.RenderTransform>
                  <TranslateTransform />
                </Ellipse.RenderTransform>
              </Ellipse>
              <Ellipse x:Name="PART_Hit0" Width="20" Height="20" Fill="{TemplateBinding Foreground}" RenderTransformOrigin="0.5, 0.5" Margin="-10" Opacity="0.3">
                <Ellipse.RenderTransform>
                  <TransformGroup>
                    <ScaleTransform ScaleX="0" ScaleY="0" />
                    <TranslateTransform X="56" Y="53" />
                  </TransformGroup>
                </Ellipse.RenderTransform>
              </Ellipse>
              <Ellipse x:Name="PART_Hit1" Width="20" Height="20" Fill="{TemplateBinding Foreground}" RenderTransformOrigin="0.5, 0.5" Margin="-10" Opacity="0.3">
                <Ellipse.RenderTransform>
                  <TransformGroup>
                    <ScaleTransform ScaleX="0" ScaleY="0" />
                    <TranslateTransform X="24" Y="53" />
                  </TransformGroup>
                </Ellipse.RenderTransform>
              </Ellipse>
            </Canvas>
          </Border>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style x:Key="ChasingDotsProgressRingStyle" TargetType="c:ModernProgressRing">
    <Setter Property="Background" Value="Transparent" />
    <Setter Property="Foreground" Value="{DynamicResource Accent}" />
    <Setter Property="IsHitTestVisible" Value="False" />
    <Setter Property="HorizontalAlignment" Value="Center" />
    <Setter Property="VerticalAlignment" Value="Center" />
